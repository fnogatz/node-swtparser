{
  "author": "Falco Nogatz <fnogatz@gmail.com>",
  "name": "swtparser",
  "description": "Parser for Swiss-Chess Tournament (SWT) files",
  "keywords": ["chess", "swt", "swiss-chess"],
  "version": "1.0.2",
  "main": "lib/index.js",
  "engines": {
    "node": ">=0.8"
  },
  "devDependencies": {
    "async": "0.2.x",
    "csv": "0.3.x",
    "browserbuild": "0.6.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fnogatz/node-swtparser.git"
  },
  "bugs": {
    "url": "http://github.com/fnogatz/node-swtparser/issues"
  },
  "scripts": {
    "prepublish": "npm run-script build-structure",
    "build-structure": "node build-structure.js > lib/structure.json",
    "build-browser": "./node_modules/browserbuild/bin/browserbuild -m from-data-view -g parseSWT -b lib/ lib/from-data-view.js lib/structure.json > swtparser.browser.js"
  }
}