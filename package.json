{
  "author": "Falco Nogatz <fnogatz@gmail.com>",
  "name": "swtparser",
  "description": "Parser for Swiss-Chess Tournament (SWT) files",
  "keywords": ["chess", "swt", "swiss-chess"],
  "version": "1.0.0",
  "main": "lib/index.js",
  "engines": {
    "node": ">=0.8"
  },
  "devDependencies": {
    "async": "0.1.x",
    "csv": "0.0.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/chessio/node-swtparser.git"
  },
  "bugs": {
    "url": "http://github.com/chessio/node-swtparser/issues"
  },
  "scripts": {
    "prepublish": "npm run-script build-structure",
    "build-structure": "node build-structure.js > lib/structure.json",
    "build-browser": "sed -e '/Structure = require/r lib/structure.json' -e 's/Structure = require(..\\/structure.json.);/Structure = /ig' lib/from-data-view.js | sed -e ':a;N;$!ba;s/Structure = \\n/Structure = /g' > swtparser.browser.js"
  }
}